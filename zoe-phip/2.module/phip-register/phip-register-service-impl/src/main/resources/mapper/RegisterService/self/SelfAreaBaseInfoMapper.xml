<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.zoe.phip.register.dao.IAreaBaseInfoMapper">
    <resultMap id="MultiResultMap" type="com.zoe.phip.register.model.AreaBaseInfo" extends="BaseResultMap">

    </resultMap>
    <update id="defaultUpdate" parameterType="com.zoe.phip.register.model.AreaBaseInfo">
        update PHIP_AREA_BASE_INFO
        <set>
            <if test="code != null">
                CODE = #{code,jdbcType=VARCHAR},
            </if>
            <if test="name != null">
                NAME = #{name,jdbcType=VARCHAR},
            </if>
            <if test="pareaId != null">
                P_AREA_ID = #{pareaId,jdbcType=VARCHAR},
            </if>
            <if test="buildTime != null">
                BUILD_TIME = #{buildTime,jdbcType=TIMESTAMP},
            </if>
            <if test="areaCancellationDate != null">
                AREA_CANCELLATION_DATE = #{areaCancellationDate,jdbcType=TIMESTAMP},
            </if>
            <if test="areaCancelReson != null">
                AREA_CANCEL_RESON = #{areaCancelReson,jdbcType=VARCHAR},
            </if>
            <if test="historyAreaId != null">
                HISTORY_AREA_ID = #{historyAreaId,jdbcType=VARCHAR},
            </if>
            <if test="state != null">
                STATE = #{state,jdbcType=DECIMAL},
            </if>
            MODIFY_AT=sysdate
        </set>
        where id = #{id}
    </update>
</mapper>