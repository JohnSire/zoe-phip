			</annotation>
		</attribute>
		<attribute name="parentDocumentRelationship" type="s:string">
			<annotation>
				<documentation>与父文档关系类别</documentation>
			</annotation>
		</attribute>
		<attribute name="parentDocumentId" type="s:string">
			<annotation>
				<documentation>父文档唯一标识号</documentation>
			</annotation>
		</attribute>
