<schema xmlns="http://www.w3.org/2001/XMLSchema" xmlns:s="http://www.w3.org/2001/XMLSchema" xmlns:xs="urn:hl7-org:v3" targetNamespace="urn:hl7-org:v3" elementFormDefault="qualified">
	<element name="ProvideAndRegisterDocumentSetRequest">
		<complexType>
			<sequence>
				<element minOccurs="0" name="ID" type="xs:II">
					<annotation>
						<documentation>消息标识</documentation>
					</annotation>
				</element>
				<element name="SourcePatientID" type="s:string" minOccurs="0">
					<annotation>
						<documentation>患者标识</documentation>
					</annotation>
				</element>
				<element name="SourcePatientName" type="s:string">
					<annotation>
						<documentation>患者姓名</documentation>
					</annotation>
				</element>
				<element name="HealthCardId" type="s:string" minOccurs="0">
					<annotation>
						<documentation>患者健康卡号</documentation>
					</annotation>
				</element>
				<element name="IdentityId" type="s:string">
					<annotation>
						<documentation>患者身份证号</documentation>
					</annotation>
				</element>
				<element name="Organization" type="xs:Organization" nillable="true" minOccurs="0" maxOccurs="unbounded">
					<annotation>
						<documentation>健康机构信息</documentation>
					</annotation>
				</element>
				<element name="RegistryPackage" type="xs:RegistryPackage">
					<annotation>
						<documentation>共享文档注册条目包</documentation>
					</annotation>
				</element>
				<element name="Document" type="xs:DocumentEntry" maxOccurs="unbounded">
					<annotation>
						<documentation>提交共享文档内容</documentation>
					</annotation>
				</element>
			</sequence>
		</complexType>
	</element>
	<complexType name="II">
		<attribute name="root" type="s:string">
			<annotation>
				<documentation>标识符根</documentation>
			</annotation>
		</attribute>
		<attribute name="extension" type="s:string">
			<annotation>
				<documentation>根下唯一标识</documentation>
			</annotation>
		</attribute>
	</complexType>
	<complexType name="Organization">
		<sequence>
			<element name="Name" type="s:string" minOccurs="0">
				<annotation>
					<documentation>名称</documentation>
				</annotation>
			</element>
			<element name="TelephoneNumber" type="xs:Telephone" minOccurs="0">
				<annotation>
					<documentation>联系电话</documentation>
				</annotation>
			</element>
			<element name="EmailAddress" type="xs:Email" minOccurs="0">
				<annotation>
					<documentation>电子邮件</documentation>
				</annotation>
			</element>
			<element name="Address" type="xs:Address" minOccurs="0">
				<annotation>
					<documentation>联系地址</documentation>
				</annotation>
			</element>
		</sequence>
