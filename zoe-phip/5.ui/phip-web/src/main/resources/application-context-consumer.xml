<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
	   http://code.alibabatech.com/schema/dubbo http://code.alibabatech.com/schema/dubbo/dubbo.xsd
	   ">
    <dubbo:application name="zoe-phip-consumer"/>

    <dubbo:registry address="N/A"/>

    <!--地址：本地测试时用localhost 部署时要换成Ip地址-->
    <dubbo:reference id="SystemUserService" interface="com.zoe.phip.service.in.sm.SystemUserService"
                     url="dubbo://localhost:20080/com.zoe.phip.service.in.sm.SystemUserService"/>
    <dubbo:reference id="SystemDictCategoryService" interface="com.zoe.phip.service.in.sm.SystemDictCategoryService"
                     url="dubbo://localhost:20080/com.zoe.phip.service.in.sm.SystemDictCategoryService"/>
    <dubbo:reference id="SystemDictItemService" interface="com.zoe.phip.service.in.sm.SystemDictItemService"
                     url="dubbo://localhost:20080/com.zoe.phip.service.in.sm.SystemDictItemService"/>
    <dubbo:reference id="MenuDataService" interface="com.zoe.phip.service.in.sm.MenuDataService"
                     url="dubbo://localhost:20080/com.zoe.phip.service.in.sm.MenuDataService"/>
    <dubbo:reference id="SystemParameterService" interface="com.zoe.phip.service.in.sm.SystemParameterService"
                     url="dubbo://localhost:20080/com.zoe.phip.service.in.sm.SystemParameterService"/>


</beans>
