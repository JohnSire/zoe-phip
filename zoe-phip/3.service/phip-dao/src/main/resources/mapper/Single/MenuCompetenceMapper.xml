<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.zoe.phip.dao.sm.MenuCompetenceMapper">
    <resultMap id="BaseResultMap" type="com.zoe.phip.model.sm.MenuCompetence">
        <id column="ID" property="id" javaType="java.lang.String"
            jdbcType="VARCHAR"/>
        <result column="CREATE_AT" property="createAt" javaType="java.sql.Timestamp"
                jdbcType="TIMESTAMP"/>
        <result column="CREATE_BY" property="createBy" javaType="java.lang.String"
                jdbcType="VARCHAR"/>
        <result column="FK_COMPETENCE_CATEGORY_ID" property="fkCompetenceCategoryId" javaType="java.lang.String"
                jdbcType="VARCHAR"/>
        <result column="FK_MENU_ID" property="fkMenuId" javaType="java.lang.String"
                jdbcType="VARCHAR"/>
    </resultMap>
    <insert id="addList" parameterType="java.util.List">
        INSERT INTO SYS_MENU_COMPETENCE (ID,
        CREATE_AT,
        CREATE_BY,
        FK_COMPETENCE_CATEGORY_ID,
        FK_MENU_ID)
        <foreach collection="list" item="item" index="index" close="" open="" separator="union all">
            SELECT
            #{item.id,jdbcType=VARCHAR},
            #{item.createAt,jdbcType=TIMESTAMP},
            #{item.createBy,jdbcType=VARCHAR},
            #{item.fkCompetenceCategoryId,jdbcType=VARCHAR},
            #{item.fkMenuId,jdbcType=VARCHAR} FROM DUAL
        </foreach>
    </insert>
    <delete id="deleteByIds" parameterType="java.util.List">
        delete from SYS_MENU_COMPETENCE where ID in
        <foreach collection="list" item="item" open="(" separator="," close=")">
            #{item}
        </foreach>
    </delete>
    <delete id="deleteByList" parameterType="java.util.List">
        delete from SYS_MENU_COMPETENCE where ID in
        <foreach collection="list" item="item" open="(" separator="," close=")">
            #{item.id}
        </foreach>
    </delete>
</mapper>